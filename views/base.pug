doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    meta(http-equiv='Content-Language' content='en')
    meta(http-equiv='Content-Type' content='text/html; charset=utf-8')
    title Expresso | CMS
    meta(name='viewport' content='width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no,shrink-to-fit=no')
    meta(name='description' content='Expresso CMS')
    meta(name='msapplication-tap-highlight' content='no')
    link(href='./template/main.css' rel='stylesheet')
    link(rel='stylesheet' href='./template/bootstrap-overrides.css')
    link(rel='stylesheet' href='./template/template-overrides.css')
  body
    .app-container.app-theme-white.body-tabs-shadow.fixed-sidebar.fixed-header
      .app-header.header-shadow
        .app-header__logo
          .logo-src
            h5 Expresso CMS
          .header__pane.ml-auto
            div
              button.hamburger.close-sidebar-btn.hamburger--elastic(type='button' data-class='closed-sidebar')
                span.hamburger-box
                  span.hamburger-inner
        .app-header__mobile-menu
          div
            button.hamburger.hamburger--elastic.mobile-toggle-nav(type='button')
              span.hamburger-box
                span.hamburger-inner
        .app-header__menu
          span
            button.btn-icon.btn-icon-only.btn.btn-primary.btn-sm.mobile-toggle-header-nav(type='button')
              span.btn-icon-wrapper
                i.fa.fa-ellipsis-v.fa-w-6
        .app-header__content
          .app-header-left
            .search-wrapper
              .input-holder
                input.search-input(type='text' placeholder='Type to search')
                button.search-icon
                  span
              button.close
          .app-header-right
            .header-btn-lg.pr-0
              .widget-content.p-0
                .widget-content-wrapper
                  .widget-content-left.mr-3.header-user-info
                    .widget-heading.
                      
                      Mahmoud Khattab
                      
                    .widget-subheading.
                      
                      Admin
                      
                  .widget-content-left
                    .btn-group
                      a.p-0.btn(data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                        img.rounded-circle(width='42' src='./template/assets/images/anonymous.jpg' alt)
                        i.fa.fa-angle-down.ml-2.opacity-8
                      .dropdown-menu.dropdown-menu-right(tabindex='-1' role='menu' aria-hidden='true')
                        button.dropdown-item(type='button' tabindex='0') User Account
                        button.dropdown-item(type='button' tabindex='0') Settings
                        .dropdown-divider(tabindex='-1')
                        button.dropdown-item(type='button' tabindex='0') Logout
      .app-main
        .app-sidebar.sidebar-shadow
          .app-header__logo
            .logo-src
            .header__pane.ml-auto
              div
                button.hamburger.close-sidebar-btn.hamburger--elastic(type='button' data-class='closed-sidebar')
                  span.hamburger-box
                    span.hamburger-inner
          .app-header__mobile-menu
            div
              button.hamburger.hamburger--elastic.mobile-toggle-nav(type='button')
                span.hamburger-box
                  span.hamburger-inner
          .app-header__menu
            span
              button.btn-icon.btn-icon-only.btn.btn-primary.btn-sm.mobile-toggle-header-nav(type='button')
                span.btn-icon-wrapper
                  i.fa.fa-ellipsis-v.fa-w-6
          .scrollbar-sidebar
            .app-sidebar__inner
              ul.vertical-nav-menu
                li.app-sidebar__heading Home
                li
                  a.mm-active(href='index.html')
                    i.metismenu-icon.pe-7s-graph2
                    p Main Dashboard
                li.app-sidebar__heading Data
                li
                  a(href='#')
                    i.metismenu-icon.pe-7s-map-marker
                    p Locations
                    i.metismenu-state-icon.pe-7s-angle-down.caret-left
                  ul
                    li
                      a(href='#')
                        i.metismenu-icon
                        p Countries
                    li
                      a(href='#')
                        i.metismenu-icon
                        p Cities
                    li
                      a(href='#')
                        i.metismenu-icon
                        p Areas
                li
                  a(href='#')
                    i.metismenu-icon.pe-7s-home
                    p Restaurants
                    i.metismenu-state-icon.pe-7s-angle-down.caret-left
                  ul
                    li
                      a(href='#')
                        i.metismenu-icon
                        p Tags
                    li
                      a(href='#')
                        i.metismenu-icon
                        p Restaurants
                    li
                      a(href='#')
                        i.metismenu-icon
                        p Branches
                li
                  a(href='#')
                    i.metismenu-icon.pe-7s-add-user
                    p Users
                    i.metismenu-state-icon.pe-7s-angle-down.caret-left
                  ul
                    li
                      a(href='#')
                        i.metismenu-icon
                        p Admins
        .app-main__outer
          .app-main__inner
            //  START MAIN 
            .container-fluid
              .row
                .col
                  h1 HOME
            //  END MAIN 
          .app-wrapper-footer
            .app-footer
              .app-footer__inner
                .app-footer-right
                  ul.nav
                    li.nav-item
                      a.nav-link(href='javascript:void(0);').
                        
                        Copyright @ Expresso 2020.
                        
        script(src='http://maps.google.com/maps/api/js?sensor=true')
        script(type='text/javascript' src='./template/assets/scripts/main.js')
